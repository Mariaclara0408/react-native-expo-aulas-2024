    "splash": {
      "image": "./src/assets/bovino.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#AD9167"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#f5f5dc"
      }
    },
    "web": {
      "favicon": "./src/assets/bovino.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  },
  "plugins": [
    "expo-router",
    "expo-font"
  ]
}
