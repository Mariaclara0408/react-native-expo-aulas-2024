{
  "expo": {
    "name": "react-native-expo-2024-livia",
    "slug": "react-native-expo-2024-livia",
    "scheme": "react-native-expo-2024-livia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/bovino.jpeg",
